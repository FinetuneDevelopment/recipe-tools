{
  "name": "fine-tune-2020",
  "version": "4.2.6",
  "description": "Finetune digital design guidelines and flat HTML prototype",
  "homepage": "https://github.com/FinetuneDevelopment/fine-tune-design-guidelines",
  "scripts": {
    "build": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "watch:externalServer": "webpack --mode=development --watch --externalServer",
    "bundle": "npm ci && npm run watch",
    "bundle:externalServer": "npm ci && npm run watch:externalServer",
    "production": "cross-env NODE_ENV=production webpack --mode=production",
    "lint-sass": "sass-lint -v -q --format=compact",
    "lint-js": "eslint --ext .js src/js/"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "author": "Ross Angus https://wefinetune.com/",
  "contributors": [
    {
      "name": "Michael",
      "email": "michael@moreyum.com",
      "url": "https://www.moreyum.com/"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:WeAreAthlon/frontend-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/FinetuneDevelopment/fine-tune-design-guidelines/issues",
    "email": "ross@wefinetune.com"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "scroll-behavior-polyfill": "^2.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
